# Managed with Puppet
[logging]
# verbosity: 0 - WARN, 1- INFO, 2 - DEBUG
verbosity = 0
file = zuul-jobs-stats.log

[database]
engine = mysql
uri = <%= @zuul_jobs_stats['dburi'] %>
username = <%= @zuul_jobs_stats['dbuser'] %>
password = <%= @zuul_jobs_stats['dbpass'] %>
dbname = <%= @zuul_jobs_stats['dbname'] %>
verbose = no
